/* pages/inquireResult/inquireResult.wxss */
page{
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.info-top{
  width: 100%;
  height: 25%;
  background-color: #f0f2f5;
  border-bottom-left-radius: 100rpx;
  border-bottom-right-radius: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  line-height: 50rpx;
  font-size: 16px;
}
.info-top text{
  margin-left: 100rpx;
}
.info-mid{
  width: 80%;
  height: 20%;
  background-color: #f0f2f5;
  border-radius: 60rpx;
  margin-top: 5%;
  display: flex;
  flex-direction: column;
  
  font-size: 16px;
}
.info-mid text{
  margin-left: 100rpx;
  line-height: 50rpx;
}
.info-bot{
  width: 100%;
  height: 45%;
  background-color: #f0f2f5;
  margin-top: 5%;
  border-top-left-radius: 50rpx;
  border-top-right-radius: 50rpx;
}
.info-title{
  margin: 36rpx;
  width: 90px;
  text-align: center;
  background-color: #a85e5e;
  color: white;
  font-size: 14px;
  border-radius: 8px;
}
.dormitory-info{
  width: 650rpx;
  height: 100rpx;
  background-color: white;
  margin: 0 auto;
}
/* pages/inquireResult/inquireResult.wxss */
page{
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.info-top{
  width: 100%;
  height: 25%;
  background-color: #A46271;
  display: flex;
  flex-direction: column;
  justify-content: center;
  line-height: 50rpx;
  font-size: 16px;
  color: white;
}
.info-top text{
  margin-left: 100rpx;
}
.info-mid{
  width: 80%;
  height: 20%;
  background-color: #f0f2f5;
  border-radius: 60rpx;
  margin-top: 5%;
  display: flex;
  flex-direction: column;
  border: 1px solid #bbbbbb;
  font-size: 16px;
}
.info-mid text{
  margin-left: 100rpx;
  line-height: 50rpx;
}
.info-bot{
  width: 100%;
  height: 50%;
  background-color: #f0f2f5;
  margin-top: 5%;
  border-top-left-radius: 50rpx;
  border-top-right-radius: 50rpx;
  border: 1px solid #bbbbbb;
}
.info-title{
  margin: 36rpx;
  width: 90px;
  text-align: center;
  background-color: #a85e5e;
  color: white;
  font-size: 14px;
  border-radius: 8px;
}
.info-bot .info-title{
  width: 220rpx;
}
.dormitory-info{
  width: 650rpx;
  background-color: white;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  flex-wrap: wrap;
  font-size: 14px;
}
.dormitory-info-item{
  width:200rpx;
  padding: 30rpx 40rpx;
  display: flex;
  flex-direction: column;
}